{
  "name": "mistral-tokenizer-js",
  "version": "1.0.0",
  "description": "JS tokenizer for Mistral-based LLMs",
  "keywords": [
    "mistral",
    "llm",
    "tokenizer"
  ],
  "homepage": "https://github.com/imoneoi/mistral-tokenizer#readme",
  "bugs": {
    "url": "https://github.com/imoneoi/mistral-tokenizer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imoneoi/mistral-tokenizer.git"
  },
  "license": "MIT",
  "author": "One <imonenext@gmail.com> (https://github.com/imoneoi)",
  "type": "module",
  "exports": {
    "default": "./dist/index.module.js",
    "import": "./dist/index.module.js",
    "require": "./dist/index.cjs.js"
  },
  "main": "dist/index.cjs",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.module.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && microbundle",
    "dev": "microbundle watch",
    "lint": "eslint --ext .js,.ts .",
    "prepare": "husky install",
    "pretty": "prettier --config .prettierrc.json --check '**/*.{js,ts,json}'",
    "test": "node test-mistral-tokenizer.js"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --config .prettierrc.json --write",
      "eslint"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "microbundle": "^0.15.1",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
